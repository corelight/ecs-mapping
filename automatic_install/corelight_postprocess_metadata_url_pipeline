{
  "description": "Corelight Postprocess Metadata URL Pipeline",
  "processors" : [
    {
      "description": "Add length of url.original",
      "script" : {
        "lang" : "painless",
        "source" : "ctx.url.original_length = ctx.url.original.length()",
        "if": "ctx.containsKey('url') && ctx['url'].containsKey('original')"
      },
      "ignore_failure": true
    }
  ]
}